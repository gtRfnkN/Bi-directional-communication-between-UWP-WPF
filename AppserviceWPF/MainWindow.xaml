<Window x:Class="AppserviceWPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AppserviceWPF"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBlock Margin="0,20,0,0" TextWrapping="Wrap" Text="WPF App" VerticalAlignment="Top" HorizontalAlignment="Center" FontSize="18" FontWeight="Bold"/>
        <Button Content="Send data to UWP" HorizontalAlignment="Center" Margin="321,125,321,0" VerticalAlignment="Top" Width="150" Height="40" Click="Button_Click"/>
        <TextBlock x:Name="UUID_Text" HorizontalAlignment="Center" Margin="0,80,0,0" TextWrapping="Wrap" Text="UUID: unknown" VerticalAlignment="Top" Width="400"/>
        <TextBlock x:Name="Config_Text" HorizontalAlignment="Center" Margin="0,100,0,0" TextWrapping="Wrap" Text="App Config: unknown" VerticalAlignment="Top" Width="400"/>

    </Grid>
</Window>
